{
    "name": "content-ops-starter",
    "version": "0.1.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "test": "jest --maxWorkers=75%",
        "test:watch": "jest --watch --maxWorkers=50%",
        "test:coverage": "jest --coverage --maxWorkers=50%",
        "test:unit": "jest --selectProjects='Unit Tests (@unit)' --maxWorkers=100%",
        "test:mathematical": "jest --selectProjects='Mathematical Tests (@mathematical)' --maxWorkers=50%",
        "test:performance:unit": "jest --selectProjects='Performance Tests (@performance)' --maxWorkers=1",
        "test:integration:unit": "jest --selectProjects='Integration Tests (@integration)' --maxWorkers=25%",
        "test:visual:unit": "jest --selectProjects='Visual Tests (@visual)' --maxWorkers=1",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:mathematical:e2e": "playwright test --project='*mathematical*'",
        "test:performance:e2e": "playwright test --project='*performance*' --workers=1",
        "test:visual:e2e": "playwright test --project='*visual*' --workers=2",
        "test:integration:e2e": "playwright test --project='*integration*'",
        "test:smoke:e2e": "playwright test --project='*smoke*'",
        "test:responsive": "playwright test --project='*responsive*'",
        "test:cross-browser": "playwright test --project='chromium-*' --project='firefox-*' --project='webkit-*'",
        "test:chromium": "playwright test --project='chromium-*'",
        "test:firefox": "playwright test --project='firefox-*'",
        "test:webkit": "playwright test --project='webkit-*'",
        "test:quick": "npm run test:unit && npm run test:smoke:e2e",
        "test:dev": "npm run test:unit && npm run test:mathematical && npm run test:integration:unit",
        "test:full": "npm run test:coverage && npm run test:e2e && npm run test:performance:unit",
        "test:regression": "npm run test:coverage && npm run test:cross-browser && npm run test:performance:unit && npm run test:visual:e2e",
        "test:ci": "npm run test:coverage && npm run test:mathematical:e2e && npm run test:performance:e2e",
        "test:ci:quick": "npm run test:unit && npm run test:mathematical && npm run test:smoke:e2e",
        "test:ci:full": "npm run test:regression",
        "test:performance": "npm run test:performance:unit && npm run test:performance:e2e",
        "test:performance:isolated": "npm run test:performance:unit -- --runInBand && npm run test:performance:e2e -- --workers=1",
        "test:mathematical:full": "npm run test:mathematical && npm run test:mathematical:e2e",
        "test:mathematical:validation": "npm run test:mathematical -- --testNamePattern='validation|accuracy|precision'",
        "test:visual": "npm run test:visual:unit && npm run test:visual:e2e",
        "test:visual:update": "npm run test:visual:e2e -- --update-snapshots",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
        "test:debug:e2e": "playwright test --debug",
        "test:report": "playwright show-report",
        "test:all": "npm run test:full",
        "index-content": "node scripts/index-content.js",
        "convert-notebook": "node scripts/convert-notebook.js InteractiveCayley.ipynb public/notebooks/InteractiveCayley.html",
        "build-notebooks": "npm run convert-notebook && cp InteractiveCayley.ipynb public/ && cp setup_cayley_explorer.sh public/",
        "test:caching-performance": "node scripts/test-caching-performance.js",
        "analyze:bundle": "ANALYZE=true npm run build"
    },
    "dependencies": {
        "@algolia/autocomplete-js": "^1.17.1",
        "@algolia/autocomplete-theme-classic": "^1.17.1",
        "@heroicons/react": "^2.2.0",
        "@react-spring/web": "^10.0.1",
        "@react-three/drei": "^10.6.1",
        "@react-three/fiber": "^9.3.0",
        "@types/three": "^0.178.1",
        "@use-gesture/react": "^10.3.1",
        "algoliasearch": "^4.24.0",
        "canvas-confetti": "^1.9.3",
        "chart.js": "^4.5.0",
        "classnames": "^2.5.1",
        "d3": "^7.9.0",
        "dayjs": "^1.11.11",
        "dexie": "^4.0.11",
        "dexie-export-import": "^4.1.4",
        "dotenv": "^16.6.1",
        "flexsearch": "^0.8.205",
        "framer-motion": "^12.23.12",
        "front-matter": "^4.0.2",
        "glob": "^10.4.2",
        "katex": "^0.16.22",
        "markdown-to-jsx": "^7.7.3",
        "marked": "^14.1.2",
        "mermaid": "^10.9.3",
        "next": "15.3.3",
        "next-mdx-remote": "^5.0.0",
        "plotly.js": "^2.35.3",
        "prismjs": "^1.30.0",
        "react": "^19.1.0",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.1.0",
        "react-katex": "^3.1.0",
        "react-plotly.js": "^2.6.0",
        "react-syntax-highlighter": "^15.6.1",
        "rehype-katex": "^7.0.1",
        "remark-math": "^6.0.0",
        "swiper": "^11.1.4",
        "tailwindcss": "^3.4.3",
        "three": "^0.178.0",
        "zustand": "^5.0.7"
    },
    "devDependencies": {
        "@base2/pretty-print-object": "^1.0.2",
        "@next/bundle-analyzer": "^15.4.6",
        "@playwright/test": "^1.48.0",
        "@stackbit/cms-git": "^1.0.32",
        "@stackbit/types": "^2.1.10",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/d3": "^7.4.3",
        "@types/dexie": "^1.3.32",
        "@types/flexsearch": "^0.7.6",
        "@types/glob": "^8.1.0",
        "@types/katex": "^0.16.7",
        "@types/plotly.js": "^2.35.7",
        "@types/prismjs": "^1.26.5",
        "@types/react": "^19.1.0",
        "@types/react-dom": "^19.1.1",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.19",
        "babel-jest": "^29.7.0",
        "eslint": "^9.33.0",
        "eslint-plugin-react": "^7.37.5",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "pixelmatch": "^5.3.0",
        "pngjs": "^7.0.0",
        "prettier": "^3.3.2",
        "typescript": "^5.6.2",
        "vitest": "^2.1.3"
    },
    "main": "next.config.js"
}
